package task5_ques2;

public class ques2 {

	public static void main(String[] args) {
		String str  =  "Consultadd pvt";
		int n = str.length(); 
         String rev ="", ans="";
          char ch;
          
         for(int i=0;i<n;i++){

	         ch= str.charAt(i);
	         if(ch!=' ') {
             rev = ch+rev;
	         }
	         else{
	        	ans= ans+rev;
	        	ans+=" ";
	        	rev="";
                
	       }

	       System.out.println("Reversed string: "+ans);

	}

	}

}
