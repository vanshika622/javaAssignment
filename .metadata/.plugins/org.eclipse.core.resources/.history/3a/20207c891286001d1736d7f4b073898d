package task5_ques2;

public class ques2 {

	public static void main(String[] args) {
		String str = "Consultadd pvt";
        char[] tmp = str.toCharArray();
        int left=0, right = tmp.length - 1;
  
        while(left<right){
            // Swap values of left and right
            char temp = tmp[left];
            tmp[left] = tmp[right];
            tmp[right] = temp;
            left++;
            right--;
        }
  
        for (char c : tmp)
            System.out.print(c);
        System.out.println();

	}

}
